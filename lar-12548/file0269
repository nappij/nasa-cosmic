PROGRAM EXAMPLE (OUTPUT) ;
BEGIN
   REPEAT
      X %= 2 ;
      Y %= X + 1 ;
   UNTIL (I > MAX) ;
   CASE SWITCH OF
      1% DOONCE (1) ; 
      2% DOTWICE (2) ;
      3% DOTHRICE (3) 
   END ;
   IF (NOT RAINING) THEN PICNIC ELSE SLEEP ;
   IF (SUPERRICH) THEN PRIVATECHEF
   ELSE IF (VERYRICH) THEN RUSSIANTEAROOM
   ELSE IF (RICH) THEN ANTHONYPIERFOUR
   ELSE IF (HIP) THEN FELICIA
   ELSE IF (CHIC) THEN JASONS
   ELSE IF (GOURMET) THEN COOKYOURSELF
   ELSE IF (DONTCARE) THEN MACDONALDS
   ELSE IF (OVERWEIGHT) THEN DIET
   ELSE STARVE ;
   WHILE (WATCHINGTV) DO EATJUNKFOOD ;
END (* PROGRAM *) .
