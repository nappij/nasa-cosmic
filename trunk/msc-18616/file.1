      IF (TNKT(I).LT.-460.0) TN(ISCT)=TVLE                              00538900
      GO TO 1110                                                        00539000
 1103 IFTP(ISCT)=3                                                      00539100
      GO TO 1110                                                        00539200
 1105 JS=INKT(I)                                                        00539300
      JSCT=NSCTL(JS)                                                    00539400
