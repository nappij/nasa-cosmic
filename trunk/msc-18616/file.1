     1           CM7,CM91,CM92                                          00015800
 1210 CALL TIMEV(TIME2)                                                 00015900
      TIME4=TIME2                                                       00016000
      TMS3=TIME2                                                        00016100
      IF (TITLE(14).EQ.CLOCK) ICLOCK=1                                  00016200
C                                                                       00016300
