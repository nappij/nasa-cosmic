 1000 NCASE=0                                                           00012500
      IDOMT=0                                                           00012600
      IPCRT=0                                                           00012601
      CALL OPENMS(12,IRAND,7000,0)                                      00012602
C                                                                       00012700
C     CASE LOOP                                                         00012800
 1100 CALL TIMEV(TIME1)                                                 00012900
      REWIND 2                                                          00013000
      REWIND 3                                                          00013100
      REWIND 4                                                          00013200
      REWIND 8                                                          00013300
      REWIND 9                                                          00013400
      REWIND 10                                                         00013500
      TT1=0.0                                                           00013600
      TT2=0.0                                                           00013700
      TT3=0.0                                                           00013800
      TT4=0.0                                                           00013900
      TT5=0.0                                                           00014000
      TT6=0.0                                                           00014100
      TT7=0.0                                                           00014200
      NSIZE=0                                                           00014300
      ISIZE=0                                                           00014400
      IPNSZ=0                                                           00014500
      TMS5=0.0                                                          00014600
      ICLOCK=0                                                          00014700
C                                                                       00014800
C     INPUT DATA                                                        00014900
 1200 CALL INPT                                                         00015000
      CALL BDRY                                                         00015100
      CALL INPS                                                         00015200
      CALL TBLS                                                         00015300
      IF (NCASE.NE.1) GO TO 1210                                        00015400
