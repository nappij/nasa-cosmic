  131 FORMAT(1H 5X,I4,F10.3)                                            00011900
  132 FORMAT(1H 5X,I4,10X,F10.3)                                        00012000
  133 FORMAT(1H 5X,I4,20X,F10.3)                                        00012100
C                                                                       00012200
