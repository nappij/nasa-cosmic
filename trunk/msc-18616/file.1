      IXL=IXD-I                                                         00110700
      IYB=IYD+2*(12-I)                                                  00110800
      IYM=IYB+I                                                         00110900
      IYT=IYB+3*I                                                       00111000
      DO 1010 J=1,2                                                     00111100
