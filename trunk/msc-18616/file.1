      IS=NBVT(I,IN1)                                                    00515200
      ISCT=LONP(IS,NCOLO,NCOL)                                          00515300
      IF (ISCT.EQ.0) GO TO 2050                                         00515400
      DO 2040 J=1,NAVTO                                                 00515500
      IF (BVT(I,J,IN1).NE.0.0) GO TO 2040                               00515600
