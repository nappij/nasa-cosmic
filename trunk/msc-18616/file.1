C       IAJ - ARRAY OF NUMBERS                                          00059600
C                                                                       00059700
C                *****      *****      *****      *****                 00059800
C                                                                       00059900
      FUNCTION LOAP(NI,NJ,NE,IAI,IAJ)                                   00060000
C                                                                       00060100
      DIMENSION IAI(1),IAJ(1)                                           00060200
C                                                                       00060300
 1000 LOAP=0                                                            00060400
      IF (NE.LE.0) GO TO 1090                                           00060500
      NBI=IABS(NI)                                                      00060600
      NBJ=IABS(NJ)                                                      00060700
