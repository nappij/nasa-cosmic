 1090 CONTINUE                                                          00537300
      IF (IPTT.LT.0) GO TO 1095                                         00537400
      IF (IPNT.EQ.1.OR.IPNT.EQ.2) NSPTO=0                               00537500
 1095 ICOND=-1                                                          00537600
      CALL COND                                                         00537700
 1100 IF (NKTO.EQ.0) GO TO 1200                                         00537800
