      SUBROUTINE PNCH                                                   00762800
C                                                                       00762900
      COMMON /CMN1/ NCASE,INPO(36),INRO(36),DELT,DELTP,TIMEO,TEND,      00763000
     1              FTRN,DMTX,TOLR,RCMIN,DELTC,TIMEC,TIMEP,             00763100
