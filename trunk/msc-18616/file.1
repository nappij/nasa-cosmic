      FUNCTION MSTD(IB,IS,IT,ISG,IRG,ASG,ARG)                           00064300
C                                                                       00064400
      EQUIVALENCE (IARG,AARG)                                           00064500
C                                                                       00064600
      DATA IBLK / 77777777777777777777B /                               00064700
      DATA MASK / 77770000000000000000B /                               00064701
